<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ScrollPane?>

<ScrollPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.clinica.application.ViewMainControler">
   <content>
      <MenuBar prefHeight="25.0" prefWidth="371.0">
        <menus>
          <Menu mnemonicParsing="false" text="Cadastros">
            <items>
              <MenuItem fx:id="menuItemCadastroCidade" mnemonicParsing="false" onAction="#onMenuItemCadastroCidade" text="Cidades" />
              <MenuItem fx:id="menuItemCadastroCliente" mnemonicParsing="false" onAction="#onMenuItemCadastroCliente" text="Clientes" />
              <MenuItem fx:id="menuItemCadastroServico" mnemonicParsing="false" onAction="#onMenuItemCadastroServico" text="Serviços" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Agendamentos">
            <items>
              <MenuItem fx:id="menuItemAgendamento" mnemonicParsing="false" onAction="#onMenuItemAgendamento" text="Novo Agendamento" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Relatórios">
            <items>
              <MenuItem fx:id="menuItemRelatorioCidade" mnemonicParsing="false" onAction="#onMenuItemRelatorioCidade" text="Cidades" />
              <MenuItem fx:id="menuItemRelatorioCliente" mnemonicParsing="false" onAction="#onMenuItemRelatorioCliente" text="Clientes" />
              <MenuItem fx:id="menuItemRelatorioServico" mnemonicParsing="false" onAction="#onMenuItemRelatorioServico" text="Serviços" />
              <MenuItem fx:id="menuItemRelatorioAgendamento" mnemonicParsing="false" onAction="#onMenuItemRelatorioAgendamento" text="Agendamentos" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </content>
</ScrollPane>
